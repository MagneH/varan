{
  "name": "varan",
  "version": "0.1.1",
  "description": "A webpack starter kit for bring-your-own-code apps with server side rendering",
  "author": "ersims",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ersims/varan.git"
  },
  "keywords": [
    "varan",
    "create-react-app",
    "react",
    "webpack",
    "ssr",
    "boilerplate"
  ],
  "license": "MIT",
  "readmeFilename": "README.md",
  "bugs": {
    "url": "https://github.com/ersims/varan/issues"
  },
  "homepage": "https://github.com/ersims/varan#README.md",
  "engines": {
    "node": ">= 8.10.0",
    "npm": ">= 5.6.0"
  },
  "main": "index.js",
  "bin": {
    "varan": "./cli.js"
  },
  "scripts": {
    "apply:format": "prettier --config .prettierrc --write \"{babel,examples,src,test,webpack}/**/*.js?(x)\"",
    "check:format": "prettier --config .prettierrc --list-different \"{babel,examples,src,test,webpack}/**/*.js?(x)\"",
    "check": "npm-run-all check:*",
    "clean": "del-cli ./.coverage",
    "pretest": "npm run check",
    "test": "jest"
  },
  "dependencies": {
    "@babel/core": "^7.0.0-beta.44",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.44",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.44",
    "@babel/plugin-transform-react-constant-elements": "^7.0.0-beta.44",
    "@babel/plugin-transform-react-inline-elements": "^7.0.0-beta.44",
    "@babel/plugin-transform-react-jsx-self": "^7.0.0-beta.44",
    "@babel/plugin-transform-react-jsx-source": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "@babel/preset-react": "^7.0.0-beta.44",
    "babel-loader": "^8.0.0-beta.2",
    "commander": "^2.15.0",
    "connect-history-api-fallback": "^1.5.0",
    "css-loader": "^0.28.11",
    "detect-port-alt": "^1.1.6",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "http-proxy-middleware": "^0.18.0",
    "koa-connect": "^2.0.1",
    "lodash.defaults": "^4.2.0",
    "lodash.omit": "^4.5.0",
    "node-sass": "^4.9.0",
    "react-dev-utils": "^5.0.1",
    "resolve-url-loader": "^2.3.0",
    "sass-loader": "^7.0.1",
    "shelljs": "^0.8.1",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "validate-npm-package-name": "^3.0.0",
    "webpack": "^4.4.1",
    "webpack-hot-client": "^2.2.2",
    "webpack-manifest-plugin": "^2.0.1",
    "webpack-merge": "^4.1.2",
    "webpack-node-externals": "^1.7.2",
    "webpack-serve": "^0.3.1",
    "webpack-stats-plugin": "^0.2.1"
  },
  "devDependencies": {
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^22.4.3",
    "del-cli": "^1.1.0",
    "express": "^4.16.3",
    "jest": "^22.4.3",
    "memory-fs": "^0.4.1",
    "normalize.css": "^8.0.0",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.12.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.1.2",
    "react-test-renderer": "^16.3.2",
    "semver": "^5.5.0"
  },
  "jest": {
    "bail": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js?(x)",
      "webpack/**/*.js?(x)",
      "*.js"
    ],
    "coverageDirectory": ".coverage",
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    },
    "testMatch": [
      "<rootDir>/test/__tests__/**/*.js?(x)",
      "<rootDir>/examples/**/test/__tests__/**/*.js?(x)"
    ],
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/test/fixtures/jestPreprocessor.js"
    },
    "moduleNameMapper": {
      "./clearConsole": "<rootDir>/test/__mocks__/react-dev-utils/clearConsole.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|ico)$": "<rootDir>/test/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/test/__mocks__/styleMock.js"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/examples/(.*)/test/__mocks__"
    ],
    "testEnvironment": "node",
    "silent": true,
    "verbose": true
  }
}
